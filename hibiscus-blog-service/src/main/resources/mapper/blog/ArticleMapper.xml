<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="gold.hibiscus.blog.infrastructure.persistence.mybatis.mapper.blog.ArticleMapper">
    <select id="queryArticleList" resultType="gold.hibiscus.blog.domain.blog.Article">
        SELECT id, title, content, author, create_date, update_date
        FROM article
    </select>
</mapper>