<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="gold.hibiscus.blog.infrastructure.persistence.mybatis.mapper.blog.ArticleMapper">
    <select id="selectArticleByArticleId" resultType="gold.hibiscus.blog.infrastructure.persistence.mybatis.dataobject.blog.ArticleDO">
        select
        id,
        article_id,
        title,
        content,
        author,
        create_time,
        update_time
        from
        article
        where
        article_id = #{articleId}
</mapper>